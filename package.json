{
  "name": "delights",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "production:build": "pnpx prisma generate && pnpx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.2.0"
  }
}
